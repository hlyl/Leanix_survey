# LeanIX Configuration
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# LeanIX API Endpoint
LEANIX_URL=https://your-instance.leanix.net

# LeanIX API Token (Bearer token for authentication)
LEANIX_API_TOKEN=your-leanix-api-token-here

# LeanIX Workspace ID (UUID)
LEANIX_WORKSPACE_ID=your-workspace-uuid-here

# Backend Configuration
BACKEND_URL=http://api:8000

# Logging Level
LOG_LEVEL=INFO

# Caching Configuration
CACHE_ENABLED=true
MAX_BATCH_SIZE=25
API_TIMEOUT=30

# Streamlit Configuration
STREAMLIT_SERVER_HEADLESS=true

# Docker Configuration
COMPOSE_PROJECT_NAME=leanix-survey
